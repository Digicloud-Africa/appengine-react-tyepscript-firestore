{
  "name": "ssr-pwa",
  "description": "An example TypeScript app running on Google App Engine.",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prepare": "npm run gcp-build",
    "pretest": "npm run gcp-build",
    "posttest": "npm run lint",
    "lint": "tslint -p . -e ./node-modules/**/*",
    "start": "ts-node ./src/index.ts",
    "gcp-build": "tsc -p .; ts-node getEnv.ts",
    "deploy": "gcloud app deploy"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^1.0.6",
    "@google-cloud/firestore": "^2.6.1",
    "@google-cloud/storage": "^4.3.0",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.8",
    "bulma": "^0.8.0",
    "express": "^4.16.3",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.0",
    "googleapis": "^45.0.0",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-head": "^3.3.0",
    "react-native": "^0.61.5"
  },
  "devDependencies": {
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/express": "^4.16.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "tslint": "^5.11.0"
  },
  "main": "src/index.js"
}
